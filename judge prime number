#include<bits/stdc++.h>
using namespace std;
int main(){
	int n;
	cin>>n;
	int a[n];
	for(int i=0;i<n;i++){
		cin>>a[i];
		if(a[i]==2){
			cout<<"Y"<<endl; 
		}
		for(int j=2;j<a[i];j++){
			if(a[i]%j==0){
				cout<<"N"<<endl;
				break;
			}
			else {
				if(j==a[i]-1)cout<<"Y"<<endl;
			}
		}	
	}
}
