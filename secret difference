#include <iostream>
#include <cmath>
#include <string>
using namespace std;
int main(){
string s;
while(cin>>s){
    int a,b,c,n;
    a=0;
    b=0;
    c=0;
    n=s.size();
    for(int i=n-1;i>=0;i--){
        c=s[i]-'0';
        if(i%2==0){
       a=a+c;
    }
    else b=b+c;
    }cout<<abs(a-b);
}
}
